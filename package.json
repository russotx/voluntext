{
  "name": "voluntext",
  "version": "1.0.0",
  "description": "app for tracking volunteer hours with help of sms",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/server.js",
    "prestart": "babel src --out-dir build --copy-files",
    "start": "node build/server.js",
    "postinstall": "npm run clean-build && babel src --out-dir build",
    "clean-build": "rm -r build && mkdir build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/russotx/voluntext.git"
  },
  "author": "Daniel Russo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/russotx/voluntext/issues"
  },
  "homepage": "https://github.com/russotx/voluntext#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "crypto-random-string": "^1.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-session": "^1.15.5",
    "moment": "^2.18.1",
    "mongodb": "^2.2.30",
    "mongoose": "^4.11.6",
    "morgan": "^1.8.2",
    "nodemailer": "^4.1.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-local": "^1.0.0",
    "request": "^2.83.0",
    "ws": "^3.1.0"
  }
}
